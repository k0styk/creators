    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.2",
